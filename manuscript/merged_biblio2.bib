@Article{	  de_tombeur_shift_2021,
  title		= {A shift from phenol to silica-based leaf defences during
		  long-term soil and ecosystem development},
  volume	= {24},
  issn		= {1461-0248},
  url		= {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.13713},
  doi		= {10.1111/ele.13713},
  abstract	= {The resource availability hypothesis predicts that plants
		  adapted to infertile soils have high levels of
		  anti-herbivore leaf defences. This hypothesis has been
		  mostly explored for secondary metabolites such as
		  phenolics, whereas it remains underexplored for
		  silica-based defences. We determined leaf concentrations of
		  total phenols and silicon (Si) in plants growing along the
		  2-million-year Jurien Bay chronosequence, exhibiting an
		  extreme gradient of soil fertility. We found that nitrogen
		  (N) limitation on young soils led to a greater expression
		  of phenol-based defences, whereas old, phosphorus
		  (P)-impoverished soils favoured silica-based defences. Both
		  defence types were negatively correlated at the community
		  and individual species level. Our results suggest a
		  trade-off among these two leaf defence strategies based on
		  the strength and type of nutrient limitation, thereby
		  opening up new perspectives for the resource availability
		  hypothesis and plant defence research. This study also
		  highlights the importance of silica-based defences under
		  low P supply.},
  pages		= {984--995},
  number	= {5},
  journaltitle	= {Ecology Letters},
  author	= {de Tombeur, Felix and Laliberté, Etienne and Lambers,
		  Hans and Faucon, Michel-Pierre and Zemunik, Graham and
		  Turner, Benjamin L. and Cornelis, Jean-Thomas and Mahy,
		  Grégory},
  urldate	= {2022-04-17},
  date		= {2021},
  langid	= {english},
  note		= {\_eprint:
		  https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.13713},
  keywords	= {plant–herbivore interactions, resource availability
		  hypothesis, phenylalanine, plant defence strategies, plant
		  phenols, plant silicon, resource limitation, silica-based
		  defence, soil fertility gradient, trade-offs},
  file		= {Full Text PDF:/home/isaac/Zotero/storage/CQT9CBB6/de
		  Tombeur et al. - 2021 - A shift from phenol to silica-based
		  leaf defences
		  .pdf:application/pdf;Snapshot:/home/isaac/Zotero/storage/2CC3EUXX/ele.html:text/html}
}

