  keywords	= {induced resistance, Glycine max, mineral nutrition, rust},
  file		= {Full Text PDF:/home/isaac/Zotero/storage/G3TA69P4/Cruz et
		  al. - 2014 - Soybean Resistance to Phakopsora pachyrhizi as
		  Aff.pdf:application/pdf}
}

