@Article{	  devanna_versatile_2021,
  title		= {Versatile role of silicon in cereals: Health benefits,
		  uptake mechanism, and evolution},
  volume	= {165},
  issn		= {0981-9428},
  url		= {https://www.sciencedirect.com/science/article/pii/S0981942821002217},
  doi		= {10.1016/j.plaphy.2021.03.060},
  shorttitle	= {Versatile role of silicon in cereals},
  abstract	= {Silicon (Si) is an omnipresent and second most abundant
		  element in the soil lithosphere after oxygen. Silicon being
		  a beneficial element imparts several benefits to the plants
		  and animals. In many plant species, including the cereals
		  the uptake of Si from the soil even exceeds the uptake of
		  essential nutrients. Cereals are the monocots which are
		  known to accumulate a high amount of Si, and reaping
		  maximum benefits associated with it. Cereals contribute a
		  high amount of Si to the human diet compared to other food
		  crops. In the present review, we have summarized
		  distribution of the dietary Si in cereals and its role in
		  the animal and human health. The Si derived benefits in
		  cereals, specifically with respect to biotic and abiotic
		  stress tolerance has been described. We have also discussed
		  the molecular mechanism involved in the Si uptake in
		  cereals, evolution of the Si transport mechanism and
		  genetic variation in the Si concentration among different
		  cultivars of the same species. Various genetic mutants
		  deficient in the Si uptake have been developed and many
		  {QTLs} governing the Si accumulation have been identified
		  in cereals. The existing knowledge about the Si biology and
		  available resources needs to be explored to understand and
		  improve the Si accumulation in crop plants to achieve
		  sustainability in agriculture.},
  pages		= {173--186},
  journaltitle	= {Plant Physiology and Biochemistry},
  shortjournal	= {Plant Physiology and Biochemistry},
  author	= {Devanna, B. N. and Mandlik, Rushil and Raturi, Gaurav and
		  Sudhakaran, Sreeja S. and Sharma, Yogesh and Sharma,
		  Shivani and Rana, Nitika and Bansal, Ruchi and Barvkar,
		  Vitthal and Tripathi, Durgesh K. and Shivaraj, S. M. and
		  Deshmukh, Rupesh},
  urldate	= {2022-04-24},
  date		= {2021-08-01},
  langid	= {english},
  keywords	= {Silicon, Abiotic stress, Biotic stress, Cereals,
		  Si-transporter},
  file		= {ScienceDirect
		  Snapshot:/home/isaac/Zotero/storage/MTMTKCBM/S0981942821002217.html:text/html}
}

