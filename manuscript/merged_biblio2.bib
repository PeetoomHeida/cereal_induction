@Article{	  kumar_silicification_2017,
  title		= {Silicification in Grasses: Variation between Different
		  Cell Types},
  volume	= {8},
  issn		= {1664-462X},
  url		= {https://www.frontiersin.org/articles/10.3389/fpls.2017.00438/full},
  doi		= {10.3389/fpls.2017.00438},
  shorttitle	= {Silicification in Grasses},
  abstract	= {Plants take up silicon as mono-silicic acid, which is
		  released to soil by the weathering of silicate minerals.
		  Silicic acid can be taken up by plant roots passively or
		  actively, and later it is deposited in its polymerized form
		  as amorphous hydrated silica. Major silica depositions in
		  grasses occur in root endodermis, leaf epidermal cells, and
		  outer epidermal cells of inflorescence bracts. Debates are
		  rife about the mechanism of silica deposition, and two
		  contrasting scenarios are often proposed to explain it.
		  According to the passive mode of silicification, silica
		  deposition is a result of silicic acid condensation due to
		  dehydration, such as during transpirational loss of water
		  from the aboveground organs. In general, silicification and
		  transpiration are positively correlated, and continued
		  silicification is sometimes observed after cell and tissue
		  maturity. The other mode of silicification proposes the
		  involvement of some biological factors, and is based on
		  observations that silicification is not necessarily coupled
		  with transpiration. In this review, we present evidence for
		  both mechanisms of silicification, and propose that the
		  deposition mechanism depends on the cell type.},
  journaltitle	= {Frontiers in Plant Science},
  shortjournal	= {Front. Plant Sci.},
  author	= {Kumar, Santosh and Soukup, Milan and Elbaum, Rivka},
  urldate	= {2021-06-02},
  date		= {2017},
  note		= {Publisher: Frontiers},
  keywords	= {grasses, Cell Wall, Inflorescence bracts, Root endodermis,
		  Silica cells, silicification mechanism, transpiration,
		  trichomes},
  file		= {Full Text PDF:/home/isaac/Zotero/storage/XR6QWTJS/Kumar et
		  al. - 2017 - Silicification in Grasses Variation between
		  Diffe.pdf:application/pdf}
}

