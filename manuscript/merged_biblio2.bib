@Article{	  huot_growthdefense_2014,
  title		= {Growth–Defense Tradeoffs in Plants: A Balancing Act to
		  Optimize Fitness},
  volume	= {7},
  issn		= {1674-2052},
  url		= {https://www.sciencedirect.com/science/article/pii/S1674205214609321},
  doi		= {10.1093/mp/ssu049},
  shorttitle	= {Growth–Defense Tradeoffs in Plants},
  abstract	= {Growth–defense tradeoffs are thought to occur in plants
		  due to resource restrictions, which demand prioritization
		  towards either growth or defense, depending on external and
		  internal factors. These tradeoffs have profound
		  implications in agriculture and natural ecosystems, as both
		  processes are vital for plant survival, reproduction, and,
		  ultimately, plant fitness. While many of the molecular
		  mechanisms underlying growth and defense tradeoffs remain
		  to be elucidated, hormone crosstalk has emerged as a major
		  player in regulating tradeoffs needed to achieve a balance.
		  In this review, we cover recent advances in understanding
		  growth–defense tradeoffs in plants as well as what is
		  known regarding the underlying molecular mechanisms.
		  Specifically, we address evidence supporting the
		  growth–defense tradeoff concept, as well as known
		  interactions between defense signaling and growth
		  signaling. Understanding the molecular basis of these
		  tradeoffs in plants should provide a foundation for the
		  development of breeding strategies that optimize the
		  growth–defense balance to maximize crop yield to meet
		  rising global food and biofuel demands.},
  pages		= {1267--1287},
  number	= {8},
  journaltitle	= {Molecular Plant},
  shortjournal	= {Molecular Plant},
  author	= {Huot, Bethany and Yao, Jian and Montgomery, Beronda L. and
		  He, Sheng Yang},
  urldate	= {2022-04-17},
  date		= {2014-08-01},
  langid	= {english},
  keywords	= {jasmonate, {PAMP}, plant growth, plant hormone, plant
		  immunity, salicylic acid},
  file		= {Full Text:/home/isaac/Zotero/storage/V3YPFMQL/Huot et al.
		  - 2014 - Growth–Defense Tradeoffs in Plants A Balancing
		  Ac.pdf:application/pdf;ScienceDirect
		  Snapshot:/home/isaac/Zotero/storage/8DYAR7M2/S1674205214609321.html:text/html}
}

